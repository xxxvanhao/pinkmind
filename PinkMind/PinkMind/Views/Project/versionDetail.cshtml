
@{
    ViewBag.Title = "versionDetail";
}

<link href="~/Content/css/pro_setting.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">

<div class="project-header">
    <div class="header-set">
        <div class="header-icon">
            <i class="far fa-building"></i>
        </div>
        <div class="header-text">
            <span>DNO-fresher</span>
            <span>RSK-1</span>
        </div>
    </div>
    <div class="project-action">
        <div class="action-search">
            <form action="" method="get">
                <div class="search-form">
                    <input type="text" name="" title="Search Issue" />
                    <i class="fas fa-search" title="Search Issue"></i>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="project-setting-content row">
    <div class="project-setting-menu col-lg-3">
        <ul>
            <li>
                <b>Project Setting</b>
            </li>
            <li><a href="#" class="active">Issue type</a></li>
            <li><a href="#">Category Type</a></li>
            <li><a href="#">Version</a></li>
        </ul>
    </div>
    <div class="project-contain-main col-lg-9">
        <b class="title">
            Edit Version/Milestone
        </b>
        <div class="back">
            <a href="#"><i class="fas fa-undo-alt"></i>Back</a>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>Version / Milestone</h6>
                <input type="text" name="" value="task" />
            </div>
            <p class="text-description">i.e. 1.0-M1</p>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>Start date</h6>
                <input type="datetime" value="task" />
            </div>
            <p class="text-description">In YYYY/MM/DD format E.g. 2005/06/01</p>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>End date</h6>
                <input type="datetime" name="" value="task" />
            </div>
            <p class="text-description">In YYYY/MM/DD format E.g. 2005/06/01</p>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>Description</h6>
                <textarea class="form-control" type="datetime" name="" value="task"></textarea>
            </div>
            <p class="text-description">Short description about this Version/Milestone</p>
        </div>
        <div class="edit-submit">
            <button type="submit" class="btn btn-blue"> Submit</button>
        </div>
    </div>

</div>




