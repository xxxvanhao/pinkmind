
@{
    ViewBag.Title = "versionDetail";
}

<link href="~/Content/css/pro_setting.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">

<div class="project-header">
    <div class="header-set">
        <div class="header-icon">
            <i class="far fa-building"></i>
        </div>
        <div class="header-text">
            <span>DNO-fresher</span>
            <span>RSK-1</span>
        </div>
    </div>
    <div class="project-action">
        <div class="action-search">
            <form action="" method="get">
                <div class="search-form">
                    <input type="text" name="" title="Search Issue" />
                    <i class="fas fa-search" title="Search Issue"></i>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="project-setting-content row">
    <div class="project-setting-menu col-lg-3">
        <ul>
            <li>
                <b>Project Setting</b>
            </li>
            <li><a href="#" class="active">Issue type</a></li>
            <li><a href="#">Category Type</a></li>
            <li><a href="#">Version</a></li>
        </ul>
    </div>
    <div class="project-contain-main col-lg-9">
        <b class="title">
            genaral setting
        </b>
        <div class="back">
            <a href="#"><i class="fas fa-undo-alt"></i>Go to project</a>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>Project Name</h6>
                <input type="text" name="project-name" value="" />
            </div>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>Project Key</h6>
                <input type="datetime" name="key" value="" />
            </div>
            <p class="text-description">The project key is a unique identifier for a project. A short, concise key is recommended.</p>
            <p class="text-description">(e.g. Project name Backlog has project key BLG_2) Uppercase letters (A-Z), numbers (0-9) and underscore (_) can be used</p>
        </div>
        <div class="edit-type">
            <div class="edit-type-name">
                <h6>Formatting Rules</h6>
                <select class="form-control">
                    <option>user space setting</option>
                    <option>PinkMind</option>
                    <option>MarkDown</option>
                </select>
            </div>
            <p class="text-description">Formatting rule for the Space can be set on Space Settings.</p>
        </div>
               <div class="edit-submit">
            <button type="submit" class="btn btn-blue"> Submit</button>
        </div>
    </div>

</div>




