<link href="~/Content/css/pro_setting.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">

<div class="project-header">
    <div class="header-set">
        <div class="header-icon">
            <i class="far fa-building"></i>
        </div>
        <div class="header-text">
            <span>DNO-fresher</span>
            <span>RSK-1</span>
        </div>
    </div>
    <div class="project-action">
        <div class="action-search">
            <form action="" method="get">
                <div class="search-form">
                    <input type="text" name="" title="Search Issue" />
                    <i class="fas fa-search" title="Search Issue"></i>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="project-setting-content row">
    <div class="project-setting-menu col-lg-3">
        <ul>
            <li>
                <b>Project Setting</b>
            </li>
            <li><a href="#" class="active">Issue type</a></li>
            <li><a href="#">Category Type</a></li>
            <li><a href="#">Version</a></li>
        </ul>
    </div>
    <div class="project-contain-main col-lg-9">
        <b class="title">
            Edit issue type
        </b>
        <div class="">
            <a href="#" class="btn btn-blue">
                Add issue type
            </a>
        </div>
        <div class="Type-list">
            <table class="table">
                <thead>
                    <tr>
                        <td></td>
                        <td>List of Versions / Milestones</td>
                        <td>Start Date</td>
                        <td>End Date</td>
                        <td>Description</td>
                        <td>Delete</td>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="type-name">
                            <a href="#">Phase</a>
                        </td>
                        <td class="show-type">
                            <a>
                                <label class="type-color">Phase 1</label>
                                <label class="type-count"></label>
                            </a>
                        </td>
                        <td class="start-date">

                        </td>
                        <td class="end-date"></td>
                        <td class="description"></td>
                        <td class="delete">
                            <i title="Delete Issue">X</i>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

</div>



